{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ desc }}",
  "author": "{{ author }}",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --host localhost --inline --hot --config webpack.conf.dev.babel.js",
    "build": "cross-env NODE_ENV=production webpack --progress --profile --colors --config webpack.conf.prod.babel.js"
  },
  "keywords": [
    "webpack"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    {{#lint}}
    "babel-eslint": "^7.0.0",
    "eslint": "^3.7.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.5.3",
    {{#if_eq preset "Airbnb"}}
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.16.0",
    {{/if_eq}}
    {{/lint}}
    {{#iconfont}}
    "font-awesome-webpack": "0.0.4",
    {{/iconfont}}
    {{#if (preprocessor == "Less") || iconfont}}
    "less": "^2.7.1",
    {{/if}}
    {{#if_eq preprocessor "Less"}}
    "less-loader": "^2.2.3",
    {{/if_eq}}
    {{#if_eq preprocessor "Sass"}}
    "node-sass": "^3.9.3",
    "sass-loader": "^4.0.2",
    {{/if_eq}}
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "css-loader": "^0.25.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "file-loader": "^0.9.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "webpack-merge": "^0.14.1",
    "cross-env": "^3.0.0"
  },
  "dependencies": {
    "font-awesome": "^4.6.3"
  }
}
